<%- include("partials/header"); -%>
    <h1>HOME</h1>
    <p> <%= startingContent %> </p>
    <% content.forEach((element) => { %>
        <% let truncatePostContent = element.postContent.substring(0, 100) + "..."; %>
        <h1> <%= element.postTitle %> </h1>
        <p> <%= truncatePostContent %> <a href="posts/ <%= element.postTitle %>"> Read More</a></p>
    <% }) %>
<%- include("partials/footer"); -%>